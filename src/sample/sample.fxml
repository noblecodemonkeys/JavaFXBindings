<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="79.0" prefWidth="125.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="sample.Controller">
    <children>
        <Button fx:id="button" layoutX="24.0" layoutY="14.0" onAction="#handleButtonAction" text="Click Me!" />
        <!--TODO uncomment the line below to activate the binding in code-->
        <!--<Label fx:id="lblLabel" layoutX="24.0" layoutY="52.0" />-->
        <Label fx:id="lblLabel" text="${controller.textValue}" layoutX="24.0" layoutY="52.0" />
    </children>
</AnchorPane>
